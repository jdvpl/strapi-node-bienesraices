{
  "kind": "collectionType",
  "collectionName": "propiedades",
  "info": {
    "singularName": "propiedad",
    "pluralName": "propiedades",
    "displayName": "Propiedades",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "habitaciones": {
      "type": "integer",
      "required": true
    },
    "estacionamiento": {
      "type": "integer",
      "required": true
    },
    "wc": {
      "type": "integer",
      "required": true
    },
    "descripcion": {
      "type": "richtext"
    },
    "precio": {
      "type": "biginteger"
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categorias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::categoria.categoria",
      "mappedBy": "propiedade"
    },
    "agentes": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::agente.agente"
    }
  }
}
